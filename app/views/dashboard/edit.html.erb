<div class="main posts-new">
  <div class="container">
    <%= form_tag("/dashboard/#{@article.id}/update") do %>
      <div class="form">
        <div class="form-body">
          <p>タイトル</p>
          <input name="title" value= "<%= @article.title %>">

          <p></p>
          <%= text_field_tag name="content" ,value=@article.content %>
          <input type="submit" value="保存">
        </div>
        </div>
      </div>
    <% end %>
  </div>
</div>